{
  "name": "@csv-streamy/lib",
  "version": "0.0.0-development",
  "description": "CSV Stream Library for Node.js.",
  "keywords": [
    "csv-streamy",
    "csv",
    "stream",
    "lib"
  ],
  "type": "module",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "exports": {
    ".": {
      "import": "./build/index.js",
      "require": "./build/index.cjs",
      "types": "./build/index.d.ts"
    }
  },
  "scripts": {
    "start": "node --loader ts-node/esm src/index.ts",
    "build": "tsup",
    "watch": "vitest watch --coverage",
    "test": "vitest run --coverage",
    "semantic-release": "semantic-release"
  },
  "publishConfig": {
    "access": "public"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/keidrun/csv-streamy.git",
    "directory": "packages/csv-streamy-lib"
  },
  "author": "keidrun",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/keidrun/csv-streamy/issues"
  },
  "homepage": "https://github.com/keidrun/csv-streamy/tree/main/packages/csv-streamy-lib#README.md"
}
