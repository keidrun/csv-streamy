{
  "name": "@csv-stream/lib",
  "version": "0.0.0",
  "description": "CSV Stream Library for Node.js.",
  "keywords": [
    "csv",
    "stream"
  ],
  "type": "module",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "default": "./build/index.js"
    }
  },
  "scripts": {
    "start": "node --loader ts-node/esm src/index.ts",
    "build": "tsc",
    "watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch --coverage",
    "test-clear": "jest --clearCache",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "release": "npx semantic-release"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/keidrun/csv-stream.git",
    "directory": "packages/csv-stream-lib"
  },
  "author": "keidrun",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/keidrun/csv-stream/issues"
  },
  "homepage": "https://github.com/keidrun/csv-stream#readme"
}
